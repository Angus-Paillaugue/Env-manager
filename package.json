			"esbuild"
		]
	},
	"dependencies": {
		"@inquirer/core": "^10.1.9",
		"@inquirer/prompts": "^7.4.0",
		"@inquirer/type": "^3.0.5",
		"@sveltejs/adapter-node": "^5.2.12",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/open": "^6.2.1",
		"@types/pg": "^8.11.11",
		"@types/qrcode": "^1.5.5",
		"@types/speakeasy": "^2.0.10",
		"axios": "^1.8.3",
